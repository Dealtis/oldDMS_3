<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#0077D8" />
      <stroke
          android:width="1dp"
          android:color="#ccc" />
      </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#6db3f2"
          android:endColor="#1e69de"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#ccc" />
      </shape>
  </item>
</selector>